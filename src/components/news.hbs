{{#with news}}
<section
  id="news"
  class="news"
>
  <div class="container">
    <h1 class="news__title">Новая <span class="news__title _accent">секция</span> на смоленке</h1>

    <ul class="news-list row _double-gap">
      {{#each list}}
      <li class="news-item col-6">
        {{this}}
      </li>
      {{/each}}
    </ul>
  </div>

  <div class="container _fluid">
    <div class="row _double-gap image-list">
      <div class="col-6">
        <img
          src="/src/images/Rectangle12.png"
          alt=""
        >
        <img
          src="/src/images/Rectangle30.png"
          alt=""
        >
      </div>
      <div class="col-6">
        <img
          src="/src/images/Rectangle13.png"
          alt=""
        >
        <img
          src="/src/images/Rectangle10.png"
          alt=""
        >
      </div>
      <div class="col-12">
        <img
          src="/src/images/Rectangle32.png"
          alt=""
        >
      </div>
    </div>
  </div>
</section>
{{/with}}
