{{#with fightings}}
<section
  id="fightings"
  class="fightings"
>
  <div class="overlay">
    <div class="container">
      <h1 class="fightings__title">{{ title }}</h1>

      <div class="fightings__nearest-training">
        {{nearestTraining}} <span id="nearest-fightings-value"></span>
      </div>

      <div class="row">
        <div class="col-9">
          <ul class="sub-messages">
            {{#each subMessageList}}
            <li class="sub-message__item">{{this}}</li>
            {{/each}}
          </ul>
        </div>
        <div class="col-3">
          <a
            href=""
            class="fightings__action-button"
          >ПОДРОБНЕЕ</a>
        </div>
      </div>
    </div>
  </div>
</section>
{{/with}}
